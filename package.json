{
  "name": "untouched",
  "version": "0.1.0",
  "description": "get pristine copies of built-in objects (like Array or JSON) even if they've been modified",
  "main": "untouched.js",
  "type": "module",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "format": "prettier --write .",
    "test": "node --test test/index.js"
  },
  "keywords": [
    "untouched",
    "pristine",
    "original",
    "native"
  ],
  "author": "Evan Hahn <me@evanhahn.com> (https://evanhahn.com)",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvanHahn/untouched.git"
  },
  "bugs": "https://github.com/EvanHahn/untouched/issues",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.14.0",
    "serve-static": "^2.2.0"
  }
}
