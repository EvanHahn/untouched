{
  "name": "untouched",
  "version": "0.1.0",
  "description": "get pristine copies of built-in objects (like Array or JSON) even if they've been modified",
  "main": "src/untouched.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "format": "prettier --write .",
    "test": "./node_modules/karma/bin/karma start --single-run"
  },
  "keywords": [
    "untouched",
    "pristine",
    "original",
    "native"
  ],
  "author": "Evan Hahn <me@evanhahn.com> (https://evanhahn.com)",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvanHahn/untouched.git"
  },
  "bugs": "https://github.com/EvanHahn/untouched/issues",
  "devDependencies": {
    "eslint": "^8.13.0",
    "jasmine-core": "^4.1.0",
    "karma": "^6.3.18",
    "karma-jasmine": "^5.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.16",
    "prettier": "^2.8.0"
  }
}
